{
  "name": "PPIAS",
  "author": "Antoine Cordelois",
  "version": "1.1.0",
  "private": true,
  "description": "Open source publishing platform for open science.",
  "scripts": {
    "clean": "yarn clean:config && yarn clean:nuxt && yarn clean:cache",
    "clean:config": "rm ./config.js || :",
    "clean:nuxt": "(rm -rf ${NUXT_BUILD_DIR:-.nuxt/} || :) && (rm -rf ${NUXT_GENERATE_DIR:-dist/} || :)",
    "clean:cache": "yarn clean:cache:pwa && yarn clean:cache:nuxt",
    "clean:cache:pwa": "rm -rf ./node_modules/.cache/pwa || :",
    "clean:cache:nuxt": "rm -rf ./node_modules/.cache/nuxt || :",
    "prepare": " APP_ID=${APP_ID:-PFIAS}&&  yarn clean && echo `ls`  && echo `ls submodules` && echo `ls submodules/PFIAS`  &&  cp submodules/$APP_ID/config.js config.js && rm -rf static/* && cp -r submodules/$APP_ID/static .  && cp -r submodules/$APP_ID/generated . ",
    "dev": "yarn prepare && nuxt",
    "build": "nuxt build",
    "mock": " yarn prepare && yarn generate && yarn start",
    "start": "nuxt start",
    "deploy": "nuxt generate; aws s3 cp dist s3://ppias --recursive;aws cloudfront create-invalidation --distribution-id __invalidation-batch * E20JX3UOOUCLYC",
    "generate": "yarn prepare && nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "jest"
  },
  "dependencies": {
    "@mdi/font": "^6.2.95",
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/ackee": "^3.0.2",
