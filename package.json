    "@nuxtjs/i18n": "^7.1.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sentry": "^5.1.4",
    "@nuxtjs/style-resources": "^1.2.1",
    "core-js": "^3.15.1",
    "d3": "^7.1.1",
    "d3-array": "^3.1.1",
    "d3-scale": "^4.0.2",
    "defu": "^6.1.1",
    "lite-youtube-embed": "^0.2.0",
    "mathjax-full": "^3.2.2",
    "nuxt": "2.15.7",
    "puppeteer": "^19.1.0",
    "request": "^2.88.2",
    "vuetify": "^2.5.5"
  },
